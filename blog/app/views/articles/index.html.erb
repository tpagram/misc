<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article), class: 'article_title' %>
    </li>
  <% end %>
  <%= link_to "Create new article", new_article_path(), class: "create_article" %> <br> <br>
  <%= link_to "Tags", tags_path() %>
</ul>
